{
  "name": "bukkit-unchained-playground",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "minecraft-server": "cd spigot-example && ../graal-bin/bin/java -Dgraal.LogFile=graallog.log -jar minecraft-server.jar nogui",
    "loom": "cd loom && npm start",
    "bundler": "cd Bundler && npm start",
    "compile-java": "cd Unchained && ./gradlew build && mv ./build/libs/Unchained-1.0.jar ../spigot-example/plugins/Unchained.jar",
    "watch-javascript": "cd spigot-example/plugins/Unchained && npm run watch",
    "watch-dev-plugin": "cd spigot-example/plugins/dev_plugin && npm run watch",
    "watch-portals-plugin": "cd spigot-example/plugins/portals && npm run watch",
    "watch-dral-plugin": "cd spigot-example/plugins/dral && npm run watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "eslint": {
    "extends": "react-app"
  },
  "eslintConfig": {
    "extends": "react-app",
    "globals": {
      "Java_type": true,
      "Java": true,
      "Polyglot": true,
      "print": true,
      "loadWithNewGlobal": true
    }
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "uuid": "^8.0.0",
    "ws": "^7.4.6"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-config-react-app": "^5.0.2",
    "eslint-plugin-flowtype": "^3.13.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.15.1",
    "eslint-plugin-react-hooks": "^1.7.0"
  }
}
